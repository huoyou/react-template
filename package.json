{
  "name": "react-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.13.1",
    "dayjs": "^1.10.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "dev": "set PORT=3000 && craco start FAST_REFRESH=true",
    "build": "set GENERATE_SOURCEMAP=false && craco build",
    "analyze": "env NODE_ENV=production BUILD_ANALYZER=true yarn dev",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-add": "pretty-quick --staged",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "src/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@craco/craco": "^6.1.1",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-plugin-import": "^1.13.3",
    "chalk": "^4.1.0",
    "circular-dependency-plugin": "^5.2.2",
    "compression-webpack-plugin": "^5.0.1",
    "craco-antd": "^1.19.0",
    "craco-fast-refresh": "^1.0.5",
    "craco-less": "^1.17.1",
    "craco-vtk": "^1.0.2",
    "filemanager-webpack-plugin": "^2.0.5",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "simple-progress-webpack-plugin": "^1.1.2",
    "terser-webpack-plugin": "4.2.3",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-dashboard": "^3.3.1",
    "webpackbar": "^5.0.0-3"
  }
}
